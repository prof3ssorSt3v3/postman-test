						"exec": [
							"pm.test('Created user', function(){",
							"    pm.response.to.have.status(201);",
							"    const {data} = pm.response.json();",
							"    pm.environment.set('user1', data._id);",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
